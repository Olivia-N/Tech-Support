
public class Account {
	private String userName;
	private String firstName;
	private String lastName;
	private String email;
	private String location;
	
	public Account(String userName, String firstName, String lastName, String email, String location) {
		this.userName = userName;
		this.firstName = firstName;
		this.lastName = lastName;
		this.email = email;
		this.location = location;
	}
	
	public void setUserName(String userName) {
		if (userName != null) {
			this.userName = userName;
		} else {
			if (this.userName == null) {
				this.userName = "null";
				System.out.println("ERROR - Username must not be null.");
			}
		}
	}
	
	public void setFirstName(String firstName) {
		if (firstName != null) {
			this.firstName = firstName;
		} else {
			if (this.firstName == null) {
				this.firstName = "null";
				System.out.println("ERROR - First name must not be null.");
			}
		}
	}
	
	public void setLastName(String lastName) {
		if (lastName != null) {
			this.lastName = lastName;
		} else {
			if (this.lastName == null) {
				this.lastName = "null";
				System.out.println("ERROR - Last name must not be null.");

			}
		}
	}
	
	public void setEmail(String email) {
		if (email != null) {
			this.email = email;
		} else {
			if (this.email == null) {
				this.email = "null";
				System.out.println("ERROR - Email must not be null.");

			}
		}
	}
	
	public void setLocation(String location) {
		if (location != null) {
			this.location = location;
		} else {
			if (this.location == null) {
				this.location = "null";
				System.out.println("ERROR - location must not be null.");

			}
		}
	}
	
	public String getUserName() {
		return this.userName;
	}
	
	public String getFirstName() {
		return this.firstName;
	}
	
	public String getLastName() {
		return this.lastName;
	}
	
	public String getEmail() {
		return this.email;
	}
	
	public String getLocation() {
		return this.location;
	}
	
	public String print() {
		String str = "Username: " + getUserName() + "	First Name: " + getFirstName() + "	Last Name: " + getLastName() 
					+ "\nEmail: " + getEmail() + "	Location: " + getLocation() + "\n";
		return str;
	}
}
